{
  "name": "angular2-es6-seed",
  "version": "0.0.1",
  "description": "Seed project for angular2 apps using ES6",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willyelm/angular2-es6-seed.git"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --port 3000",
    "start": "npm run server",
    "test": "karma start karma.conf.js",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  },
  "keywords": [
    "angular2",
    "seed",
    "karma",
    "protractor"
  ],
  "author": "Williams Medina <williams.medinaa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willyelm/angular2-es6-seed/issues"
  },
  "homepage": "https://github.com/willyelm/angular2-es6-seed#readme",
  "standard": {
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "copy-webpack-plugin": "^3.0.1",
    "raw-loader": "^0.5.1",
    "standard": "^7.1.2",
    "webpack": "^2.1.0-beta.19",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "^2.2.0",
    "ie-shim": "^0.1.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12"
  }
}
